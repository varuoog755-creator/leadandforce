{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/hello",
            "destination": "/api/hello.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}