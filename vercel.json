{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/auth/:path*",
            "destination": "/frontend/api/auth/:path*"
        },
        {
            "source": "/api/:path*",
            "destination": "http://72.61.235.182:3001/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}